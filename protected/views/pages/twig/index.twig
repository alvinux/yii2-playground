<div class="box box-solid">
    <div class="box-header">
        <h3 class="box-title">Contoh list </h3>
    </div>
    <div class="box-body">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Nama</th>
                    <th>Email</th>
                    <th>Telpon</th>
                    <th>Keterangan</th>
                    <th>#</th>
                </tr>
            </thead>
            <tbody>
                {% for contact in contacts %}
                    <tr>
                        <td>{{ contact.name }}</td>
                        <td>{{ contact.email }}</td>
                        <td>{{ contact.phone }}</td>
                        <td>{{ contact.keterangan }}</td>
                        <td><a href="{{ url('site/page',{'view':'contoh-twig', 'id':contact.id}) }}">
                                <span class="glyphicon glyphicon-eye-open"></span></a></td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
</div>
